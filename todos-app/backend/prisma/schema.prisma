generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// User model

model User {
  id          String    @id @default(uuid())
  name        String
  email       String
  password    String?
  avatar      String? 
  provider    String        @default("Belance")
  date        DateTime
}

